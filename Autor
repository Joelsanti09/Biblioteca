package Biblioteca;

public class Autor {
	private int id;
	private String nombre;
	private Pais pais;
	public Autor(int id, String nombre, Pais pais) {
		super();
		this.id = id;
		this.nombre = nombre;
		this.pais = pais;
	}
	@Override
	public String toString() {
		return "Autor=" + id + ", nombre=" + nombre ;
	}
	
	

}
